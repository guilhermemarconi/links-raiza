{
  "id": "profile",
  "label": "Perfil",
  "repeatable": false,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Título",
          "placeholder": "Raíza Souto"
        }
      },
      "instagram_profile": {
        "type": "Text",
        "config": {
          "label": "@ do Instagram"
        }
      },
      "profile_image": {
        "type": "Image",
        "config": {
          "constraint": {
            "width": 300,
            "height": 300
          },
          "thumbnails": [],
          "label": "Foto de perfil"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Descrição",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,heading2,heading3"
        }
      }
    }
  }
}
